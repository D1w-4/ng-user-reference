@import "~variables.styl"
.fild-label
  display block
  color $clr-grey-600
  font-size 14px
  margin-bottom 4px
.fild-input
  display: block;
  width: 100%;
  height: 34px;
  padding: 6px 12px;
  font-size: 14px;
  line-height: 1.42857143;
  color: #555;
  border: 1px solid $clr-grey-300;
  border-radius: 2px;
  margin-bottom 8px